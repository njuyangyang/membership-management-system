<h1>Admin log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: adminlogin_path) do |f| %>

      <%= f.label :uin %>
      <%= f.text_field :uin, class: 'form-control' %>

      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
      <br/>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
